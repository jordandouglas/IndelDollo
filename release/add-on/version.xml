<package name='SIMBA' version='0.0.1'>

	<depends on='BEAST.base' atleast='2.7.3'/>
	<depends on='BEAST.app' atleast='2.7.3'/>
    <service type="beast.base.core.BEASTInterface">

        <provider classname="simba.PrunedTreeLikelihood"/>
        <provider classname="simba.PrunedBeagleTreeLikelihood"/>
        <provider classname="simba.CognatePrunedTree"/>
        <provider classname="simba.CognatePrunedTreeData"/>
        <provider classname="simba.CognatePrunedTreeLogger"/>
        <provider classname="simba.CognateBirthLogger"/>
        <provider classname="simba.CognateBirthModelInitialiser"/>
        <provider classname="simba.StochasticDolloPoissonPrior"/>
        <provider classname="simba.StochasticDolloTreeLikelihoodFast"/>
        
    </service>
	
	
</package>
